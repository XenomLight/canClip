[
  {
    "match": "**/*",
    "headers": {
      "Permissions-Policy": "camera=(self), microphone=(self)",
      "Content-Security-Policy": "default-src 'self'; media-src 'self' blob:; script-src 'self'; connect-src 'self' http://localhost:* https://icp0.io https://*.icp0.io https://icp-api.io; img-src 'self' data:; style-src * 'unsafe-inline'; style-src-elem * 'unsafe-inline'; font-src *; object-src 'none'; base-uri 'self'; frame-ancestors 'none'; form-action 'self'; upgrade-insecure-requests;"
    },
    "security_policy": "standard"

    // Uncomment and add other top-level options here if needed
    // "disable_security_policy_warning": true,
    // "allow_raw_access": false
  }
]